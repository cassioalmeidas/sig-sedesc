<%= form_with(model: almoxarifado_entrada) do |form| %>
  <% if almoxarifado_entrada.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(almoxarifado_entrada.errors.count, "error") %> impediram que essa entrada fosse realizada: </h2>

      <ul>
      <% almoxarifado_entrada.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="modal-body">
    <div class="form-group">
      <%= form.hidden_field :almoxarifado_materiais_id, value: @almoxarifado_material.id %>
      <%= form.label :quantidade, class: 'col-form-label' %>
      <%= form.number_field :quantidade, class: 'form-control' %>
      <%= form.label :numero_empenho, class: 'col-form-label' %>
      <%= form.text_field :numero_empenho, class: 'form-control' %>
    </div>
  </div>

  <div class="modal-footer">
    <%= form.submit "Salvar", class: 'btn btn-primary' %>
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
  </div>
<% end %>


